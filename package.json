{
  "name": "gitxtribe-ctf-platform",
  "version": "1.0.0",
  "description": "CTF Platform for GitXTribe Technical Club",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && npm run dev",
    "client": "cd client && npm start",
    "build": "bash build.sh",
    "install-all": "npm install && cd server && npm install && cd ../client && npm install",
    "start": "cd server && npm start",
    "postinstall": "cd server && npm install && cd ../client && npm install"
  },
  "keywords": [
    "ctf",
    "capture-the-flag",
    "gitxtribe",
    "technical-club"
  ],
  "author": "GitXTribe Technical Club",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "bcryptjs": "^2.4.3",
    "multer": "^1.4.5-lts.1",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "express-rate-limit": "^7.1.5",
    "firebase-admin": "^12.0.0"
  }
}
