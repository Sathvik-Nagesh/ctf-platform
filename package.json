{
  "name": "gitxtribe-ctf-platform",
  "version": "1.0.0",
  "description": "CTF Platform for GitXTribe Technical Club",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && npm run dev",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd server && npm install && cd ../client && npm install",
    "start": "cd server && npm start"
  },
  "keywords": [
    "ctf",
    "capture-the-flag",
    "gitxtribe",
    "technical-club"
  ],
  "author": "GitXTribe Technical Club",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "axios": "^1.11.0"
  }
}
